<form class="box" action="/county/{{county._id}}/addtrack" method="POST">
  <label>Enter walk trail details:</label>
  <div class="field is-horizontal">
    <div class="field-body">
      <div class="field">
        <input class="input" type="text" placeholder="Enter Trail Name" name="title">
      </div>
      <div class="field">
        <input class="input" type="text" placeholder="Enter Discription" name="artist">
      </div>
      <div class="field">
        <input class="input" type="text" placeholder="Enter Latitude" name="duration">
      </div>
    </div>
  </div>
  <button class="button is-primary">Add Place</button>
</form>
